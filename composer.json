{
    "name": "alish/telegram",
    "description": "a laravel package for telegram bot",
    "type": "library",
    "keywords": ["telegram", "laravel", "telegram bot"],
    "time": "2018-01-17",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Shabani",
            "email": "pyramidalish@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alish\\Telegram\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alish\\Telegram\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Alish\\Telegram\\Facades\\Telegram"
            }
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.0.0",
        "illuminate/http": "^8.0.0",
        "illuminate/routing": "^8.0.0",
        "illuminate/support": "^8.0.0",
        "illuminate/contracts": "^8.0.0",
        "guzzlehttp/guzzle": "^7.3.0"
    }
}
