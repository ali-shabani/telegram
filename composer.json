{
    "name": "alish/telegram",
    "description": "a laravel package for telegram bot",
    "type": "library",
    "keywords": ["telegram", "laravel", "telegram bot"],
    "time": "2018-01-17",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Shabani",
            "email": "pyramidalish@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alish\\Telegram\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alish\\Telegram\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Alish\\Telegram\\Facades\\TelegramFacade"
            }
        }
    },
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.7.* || 5.8.*",
        "illuminate/http": "5.7.* || 5.8.*",
        "illuminate/routing": "5.7.* || 5.8.*",
        "illuminate/support": "5.7.* || 5.8.*",
        "guzzlehttp/guzzle": "^6.3"
    }
}
